@InProceedings{Birney:Durbin:Dynam_flexi_code:ISMB1997,
  author =	 {Birney, E. and Durbin, R.},
  title =	 {Dynamite: a flexible code generating language for dynamic 
                  programming methods used in sequence comparison},
  booktitle =	 ISMB97,
  year =	 {1997},
  volume =	 {5},
  number =	 {},
  pages =	 {56-64},
  user =	 {hiller},
  annote =	 {bioinformatics},
  pmid =	 {9322016},
  abstract =	 {We have developed a code generating language, called 
                  Dynamite, specialised for the production and subsequent 
                  manipulation of complex dynamic programming methods for 
                  biological sequence comparison. From a relatively simple 
                  text definition file Dynamite will produce a variety of 
                  implementations of a dynamic programming method, including 
                  database searches and linear space alignments. The speed of 
                  the generated code is comparable to hand written code, and 
                  the additional flexibility has proved invaluable in 
                  designing and testing new algorithms. An innovation is a 
                  flexible labelling system, which can be used to annotate the 
                  original sequences with biological information. We 
                  illustrate the Dynamite syntax and flexibility by showing 
                  definitions for dynamic programming routines (i) to align 
                  two protein sequences under the assumption that they are 
                  both poly-topic transmembrane proteins, with the 
                  simultaneous assignment of transmembrane helices and (ii) to 
                  align protein information to genomic DNA, allowing for 
                  introns and sequencing error.}
}



@Inproceedings{Bac:Wil:CPM2001,
  author =	 {Rolf Backofen and Sebastian Will},
