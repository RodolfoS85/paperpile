@Misc{ note1,
  note = {{The secondary structure of an RNA sequence is
  defined as the set of allowed base pairs $P$ (here Watson-Crick
  pairs plus GU) which minimize free energy, subject to a no-knot
  condition requiring that if $(i,j)$ and $(k,l)$ are both in $P$,
  then $i<k<j$ implies $i<l<j$ (i.e.~base pairs don't cross).  The
  secondary structure is computed with our implementation
  \cite{Hofacker:94} of a dynamic programming algorithm, originally
  due to \cite{Nussinov:78,Waterman:78a,Zuker:81}, which is widely used in
  laboratories to assist in the prediction of secondary structures.
  The procedure is based on empirical energy parameters
  \cite{Turner:88,Walter:91}\nocite{ack}.}}
}



