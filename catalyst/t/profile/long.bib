@INPROCEEDINGS{bruck06,
  author = {{J}ozsef {B}ruck and {O}liver {E}benhoeh and {R}einhart {H}einrich},
  title = {{P}atterns of {I}nteractions of {R}eaction {P}airs in {M}etabolic
