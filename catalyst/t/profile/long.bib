@Article{Johnson:Lu:Chen:The_human_fibro:1991,
  author =	 {Johnson, D. E. and Lu, J. and Chen, H. and Werner, S. and 
                  Williams, L. T.},
  title =	 {{The} human fibroblast growth factor receptor genes: a 
                  common structural arrangement underlies the mechanisms for 
                  generating receptor forms that differ in their third 
