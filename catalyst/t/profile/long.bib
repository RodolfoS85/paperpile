@InProceedings{Lyngso:Pedersen:Nielsen:Metri_and_simil:ISMB1999,
  author =	 {Lyngso, R. B. and Pedersen, C. N. and Nielsen, H.},
  title =	 {Metrics and similarity measures for hidden Markov models},
  booktitle =	 ISMB99,
  year =	 {1999},
  volume =	 {},
