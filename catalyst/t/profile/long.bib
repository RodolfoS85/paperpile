@techreport{gusfield:1998,
  author =	 "D. Gusfield and J. Stoye",
  title =	 "Linear time algorithms for finding and representing
                  all the tandem repeats in a string",
  institution =	 {Department of Computer Science, UC, Davis},
  year =	 {1998},
  number =	 {CSE-98-4},
  url =		 "citeseer.nj.nec.com/gusfield98linear.html",
  user =	 {rpudimat},
  abstract =	 {A tandem repeat (or square) is a string ffff, where
                  ff is a nonempty string. We present an O(jSj)-time
                  algorithm that operates on the suffix tree T (S) for
                  a string S, finding and marking the endpoint in T
                  (S) of every tandem repeat that occurs in S. This
                  decorated suffix tree implicitly represents all
                  occurrences of tandem repeats in S, and can be used
                  to efficiently solve many questions concerning
                  tandem repeats and tandem arrays in S. This improves
                  and generalizes several prior efforts to efficiently
                  capture large subsets of tandem repeats.}
}




@TECHREPORT{tr:ReiLenMutMehKec96,
  AUTHOR =	 {Reinert, Knut and Lenhof, Hans-Peter and Mutzel,
                  Petra and Mehlhorn, Kurt and Kececioglu, John D.},
  TITLE =	 {A branch-and-cut algorithm for multiple sequence
                  alignment},
  abstract =	 {Multiple sequence alignment is an important problem
                  in computational biology. We study the Maximum Trace
                  formulation introduced by Kececioglu [Kec91]. We
                  first phrase the problem in terms of forbidden
                  subgraphs, which enables us to express Maximum Trace
                  as an integer linear-programming problem, and then
                  solve the integer linear program using methods from
                  polyhedral combinatorics. The trace polytope is the
                  convex hull of all feasible solutions to the Maximum
                  Trace problem; for the case of two sequences, we
                  give a complete characterization of this
                  polytope. This yields a polynomialtime algorithm for
                  a general version of pairwise sequence alignment
                  that, perhaps suprisingly, does not use dynamic
                  programming; this yields, for instance, a
                  nondynamic-programming algorithm for sequence
                  comparison under the 0-1 metric, which gives another
                  answer to a long-open question in the area of string
                  algorithms [PW93]. For the multiple-sequence case,
                  we derive several classes of facet-defining
                  inequalities and show that for all but one class,
                  the corresponding separation problem can be solved
                  in polynomial time. This leads to a branch-and-cut
                  algorithm for multiple sequence alignment, and we
                  report on our first computational experience. It
                  appears that a polyhedral approach to multiple
                  sequence alignment can solve instances that are
                  beyond present dynamic-programming approaches.},
  ADDRESS =	 {Saarbr{\"u}cken},
  YEAR =	 {1996},
  VOLUME =	 {96-1-028},
  INSTITUTION =	 {Max-Planck-Institut f{\"u}r Informatik},
