@TechReport{smith01:_reduc_symmet_combin_desig_probl,
  author =       {Barbara M. Smith},
  title =        {Reducing Symmetry in a Combinatorial Design Problem},
  institution =  { School of Computing},
  year =         2001,
  type =         {Research Report },
  number =       { 2001.01},
  user =	 {backofen},
  abstract =	 {A combinatorial design problem is considered which can be
modelled as a constraint satisfaction problem in several different
ways. The models all have a large number of symmetries which cause
diAEculties when searching for solutions. Different approaches to
reducing the symmetry are discussed: remodelling the problem; adding
constraints to the model at the outset; and adding constraints during
search to prevent symmetric assignments being ex- plored on
backtracking. The most successful strategy for the problem of this
paper employs a complex model with less inherent symmetry than the
others, combined with symmetry breaking during search.}
}



@techreport{rahmann2003pwm,
author = {Sven Rahmann and Tobias Mueller and Martin Vingron},
title = {On the Power of Profiles for Transcription Factor Binding Site Detection},
institution = {MPI Berlin},
number = {preprint},
user = {rpudimat},
year = {2004},
abstract = {
Transcription factor binding site (TFBS) detection plays an important role in computational biology, with applications in gene  nding and gene regulation. The sites are often modeled by gapless pro les, also known as position-weight matrices. Past research has focused on the signi cance of pro le scores (the ability to avoid false positives), but this alone is not enough: The pro le must also possess the power to detect the true positive signals. Several completed genomes are now available, and the search for TFBSs is moving to a large scale; so discriminating signal from noise becomes even more challenging. Since TFBS pro les are usually estimated from only a few experimentally con rmed instances, careful regularization is an important issue. We present a novel method that is well suited for this situation. We further develop measures that help in judging pro le quality, based on both sensitivity and selectivity of a pro le. It is shown that these quality measures can be e ciently computed, and we propose statistically well-founded methods to choose score thresholds. Our  ndings are applied to the TRANSFAC database of transcription factor binding sites. The results are disturbing: If we insist on a signi cance level of 5% in sequences of length 500, only 19% of the pro les detect a true signal instance with 95% success probability under varying background sequence compositions.
}}




@techreport{gilbert2005petrinets_to_diffeqn,
author = {David Gilbert and Monika Heiner},
title = {From Petri Nets to Dierential Equations - an Integrative Approach},
school = {University of Glasgow},
address = {Glasgow, U.K.},
year={2005},
institution = {University of Glasgow},
number = {TR-2005-208},
user={rpudimat},
abstract = {We report on the results of an investigation into the integration
of Petri nets and ordinary dierential equations (ODEs) for the
modelling and analysis of biochemical networks, and the application of
our approach to the model of the inuence of the Raf Kinase Inhibitor
Protein (RKIP) on the Extracellular signal Regulated Kinase (ERK)
signalling pathway. We show that analysis based on a discrete Petri net
model of the system can be used to derive the sets of initial concentrations
required by the corresponding continuous ordinary dierential
equation model, and no other initial concentrations produce meaningful
steady states. Altogether, this paper represents a tutorial in step-wise
modelling and analysis of larger models as well as in structured design
of ODEs.}}



