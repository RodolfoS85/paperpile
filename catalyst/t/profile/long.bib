@InProceedings{Moses:Chiang:Eisen:Phylo_motif_detec:PSB2004,
  author =	 {Moses, A. M. and Chiang, D. Y. and Eisen, M. B.},
  title =	 {Phylogenetic motif detection by expectation-maximization on 
                  evolutionary mixtures},
  booktitle =	 "PSB04",
  year =	 {2004},
  volume =	 {},
  number =	 {},
  pages =	 {324-35},
  user =	 {rpudimat},
  annote =	 {bioinformatics},
  pmid =	 {14992514},
  abstract =	 {The preferential conservation of transcription factor 
                  binding sites implies that non-coding sequence data from 
                  related species will prove a powerful asset to motif 
                  discovery. We present a unified probabilistic framework for 
                  motif discovery that incorporates evolutionary information. 
                  We treat aligned DNA sequence as a mixture of evolutionary 
                  models, for motif and background, and, following the example 
                  of the MEME program, provide an algorithm to estimate the 
                  parameters by Expectation-Maximization. We examine a variety 
                  of evolutionary models and show that our approach can take 
                  advantage of phylogenic information to avoid false positives 
                  and discover motifs upstream of groups of characterized 
                  target genes. We compare our method to traditional motif 
                  finding on only conserved regions. An implementation will be 
                  made available at http://rana.lbl.gov.}
}



