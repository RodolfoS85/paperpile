@InProceedings{Heitsch2003,
  author =	 {Christine E. Heitsch},
  title =	 {The Global Arrangement of Local Reconfigurations
                  under an RNA Secondary Structure Design Procedure},
  year =	 {2003},
  booktitle =	 {Thirty-Fifth Annual ACM Symposium on Theory of
                  Computing (STOC03)},
  abstract =	 {The fundamental problem confronting attempts to
                  design RNA sequences with specific base pairings in
                  their secondary structures is preventing the
                  formation of alternate foldings with lower minimal
                  free energy. Under the given design strategy, it is
                  possible to analyze the potential reconfigurations
                  through their abstract graphical representation. In
                  this context, we define a new relation, the local
                  move, on plane trees representing RNA secondary
                  structures. We then prove that this induces an
                  intriguing new combinatorial structure on the
                  equivalence class of plane trees with
                  edges. Specifically, it is shown to be a
                  multipartite graph whose disjoint sets are
                  enumerated by the Narayana numbers.},
  user =	 {siebert}
}




