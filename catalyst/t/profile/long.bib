@InProceedings{Krippahl:Barahona:_psico:2000,
  author =	 {Ludwig Krippahl and Pedro Barahona},
  title =	 {{PSICO}: Combining Constraint Programming and
                  Optimisation to Solve Macromolecular Structures},
  booktitle =	 {ERCIM Working Group on Constraints / CompulogNet
                  Area on Constraint programming},
  year =	 2000,
  address =	 {Padova, Italy},
  month =	 {Jun},
  user =	 {will},
  abstract =	 {Here we present PSICO (Processing Structural
                  Information with Constraint propagation and
                  Optimisation), a mixed approach to determine
                  macromolecular structures from experimental and
                  theoretical data. We briefly focus the factors
                  motivating this work, the results obtained so far
                  and our perspectives for future developments and
                  applications. An application of PSICO to solve
                  structures compatible with distance constraints
                  obtained from Nuclear Magnetic Resonance (NMR) data
                  is currently at an advanced stage of development. We
                  compare the performance of this method with DYANA
                  ("Dynamics algorithm for NMR applications" [1]) an
                  existing commercial application based on simulated
                  annealing. These results indicate that PSICO has a
                  si gnificant performance advantage over existing
                  methods to solve this particular problem, and we
                  propose that this approach can be applied to a broad
                  range of problems of determination and prediction of
                  macromolecular structures}
}




