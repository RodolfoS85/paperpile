@MastersThesis{moore03:_beyon_rdf,
  author =       {Rebecca Moore},
  problem = "RNA-and-graph",
  title =        {Beyond parsing RDF: Validity and consistency analysis through graph matching},
  school =       {University of Melbourne, Department of Computer
                  Science and Software Engineering}, 
  year =         2003,
  type =         {Honours thesis},
  month =        {jun},
  user =	 {backofen},
  abstract =     {This thesis identi es some of the problems inherent in
                  utilising and maintaining RDF in real world
                  applications, and turns to graph theory in search of
                  solutions to these problems. The importance of graph
                  matching techniques is demonstrated as a source of
                  answers for enabling greater  exibility in working
                  with RDF. The thesis shows how detection of the
                  maximum common subgraph of two graphs can be useful in
                  the context of solving consistency and validity
                  problems in RDF, and compares and contrasts two
                  previously developed algorithms for performing this
                  task. A third, new algorithm is then implemented,
                  using properties from the previous two to form an
                  inexact, but e cient method for  nding the maximum
                  common subgraph of a pair of graphs. This algorithm
                  provides a useful tool for operating with RDF
                  ontologies.}
}



