@inproceedings{ankerst99:_neares_neigh_class_protein_datab,
    author = "Michael Ankerst and Gabi Kastenm{\"u}ller and Hans-Peter Kriegel and Thomas Seidl",
    title = "Nearest Neighbor Classification in {3D} Protein Databases",
    pages = "34--43",
  editor    = {Thomas Lengauer and
               Reinhard Schneider and
               Peer Bork and
               Douglas L. Brutlag and
               Janice I. Glasgow and
               Hans-Werner Mewes and
               Ralf Zimmer},
  booktitle     = {Proceedings of the Seventh International Conference on Intelligent
               Systems for Molecular Biology, August 6-10, 1999, Heidelberg,
               Germany},
  publisher = {AAAI},
    year = 1999,
    abstract = {In molecular databases, structural classification is a
                  basic task that can be successfully approached by
                  nearest neighbor methods. The underlying similarity
                  models consider spatial properties such as shape and
                  extension as well as thematic attributes. We introduce
                  3D shape histograms as an intuitive and powerful
                  approach to model similarity for solid objects such as
                  molecules. Errors of measurement, sampling, and
                  numerical rounding may result in small displacements
                  of atomic coordinates. These effects may be handled by
                  using quadratic form distance functions. An efficient
                  processing of similarity queries based on quadratic
                  forms is supported by a filter-refinement
                  architecture. Experiments on our 3D protein database
                  demonstrate the high classification accuracy of more
                  than 90\% and the good performance of the technique.}}
                  




