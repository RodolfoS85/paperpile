@InProceedings{SiebertECCB02,
  author =	 {Sven Siebert},
  title =	 {Multiple Local Sequence Structure Alignment of RNA
                  Sequences},
  abstract =	 {We present a new method that finds and aligns local
                  sequence structure segments in a set of unaligned
                  RNA sequences of unknown structures. Recognizing
                  secondary structural features allows us to find
                  highly probable sequence structure segments with
                  high similarities among them. There are a lot of
                  multiple sequence programs which consider and handle
                  primary sequences quite well. But they lack of
                  finding and aligning structural features of RNA
                  sequences. RNA sequences are often more conserved in
                  their secondary structure than in their primary
                  sequence. Sankoff was one of the first who proposed
                  a dynamic programming algorithm that aligns a set of
                  RNA sequences while predicting their common
                  structures. The most promising approach is the use
                  of stochastic context-free grammars (SCFG), which
