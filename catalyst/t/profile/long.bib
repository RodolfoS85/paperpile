@inproceedings{xu04:_exact_patter_match_rna_secon_struc,
Author = {Xu, Ying and Deng, Xiaotie and Wang, Lusheng},
Title = {Exact Pattern Matching for RNA Secondary Structures},
BookTitle = {Second Asia-Pacific Bioinformatics Conference (APBC2004)},
Editor = {Chen, Yi-Ping Phoebe},
Series= {CRPIT},
Address= {Dunedin, New Zealand},
Publisher = {ACS},
Volume = 29,
Pages = {257-263},
Year = 2004,
abstract = { Many RNA structures are assembled from a collection of RNA
                  motifs, which appear repeatedly and in various
                  combinations. Identification of RNA structural motifs
                  will enhance our understanding of RNA structures and
                  functions. Searching for secondary structural patterns
                  in sequence databases is the basic technique and
                  fundamental problem for extracting and identifying
                  such motifs. A number of algorithms and programs have
