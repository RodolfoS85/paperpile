@article{Bilgin:88,
title=       {Mutations in ribosomal proteins {L7/L12} perturb {EF-G}
		  and {EF-Tu} functions.},
author=      {Bilgin, N. and Kirsebom, L.A. and Ehrenberg, M. and Kurland, C.G.},
journal=     {Biochimie},
year=        {1988},
volume=      {70},
pages=       {611-8},
keywords=    {Bacterial Proteins, Comparative Study, Escherichia Coli, Guanosine Triphosphate, Kinetics, Mutation, Peptide Chain Elongation, Peptide Elongation Factor Tu, Peptide Elongation Factors, Ribosomal Proteins, Genetic Translation},
institution= {Department of Molecular Biology, Uppsala University, Sweden.},
abstract=    {In vitro cycling rates of E. coli ribosomes and of elongation factors EF-Tu
and EF-G have been obtained and these are compatible with translation rates
in vivo. We show that the rate of translocation is faster than 50 s-1 and
therefore that the EF-G function is not a rate limiting step in protein
synthesis. The in vivo phenotype of some L7/L12 mutants could be accounted
for by perturbed EF-Tu as well as EF-G functions. The S12 mutants that we
studied were, in contrast, only perturbed in their EF-Tu function, while
their EF-G interaction was not impaired in relation to wild type ribosomes.}
}
		  
