<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org" xmlns:appc="http://www.appcelerator.org">
   <id>paperpile</id>
   <name>Paperpile</name>
   <version>0.1</version>
   <window>
      <id>initial</id>
      <title>Paperpile</title>
     <url>app://index.html</url>
     <!--<url>http://google.com</url>-->
	
     
      <width>1024</width>
      <height>768</height>i

	<!--
      <width>1280</width>
      <height>1024</height>i
-->

      <fullscreen>false</fullscreen>
      <!--
      <minimizable>true</minimizable>
      <maximizable>false</maximizable>
      <max-width>900</max-width>
      <max-height>800</max-height>
      <min-width>400</min-width>
      <min-height>300</min-height>
      -->
      <closeable>true</closeable>
      <chrome>true</chrome>
   </window>
   <window>
      <id>smallscreen</id>
      <title>Titanium Small Screen Window</title>
      <url-regex>small.*.html</url-regex>
      <width>400</width>
      <height>275</height>
      <minimizable>true</minimizable>
      <maximizable>false</maximizable>
      <closeable>true</closeable>
      <fullscreen>false</fullscreen>
      <resizable>false</resizable>
      <chrome>true</chrome>
      <visible>true</visible>
   </window>
   <window>
      <id>fullscreen</id>
      <title>Titanium Full Screen Window</title>
      <url-regex>full.*.html</url-regex>
      <width>600</width>
      <height>400</height>
      <minimizable>true</minimizable>
      <maximizable>false</maximizable>
      <closeable>true</closeable>
      <fullscreen>true</fullscreen>
      <resizable>false</resizable>
      <chrome>true</chrome>
      <visible>true</visible>
   </window>
</ti:app>
